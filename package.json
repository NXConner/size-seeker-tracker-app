{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:size": "npm run build && npx vite-bundle-analyzer dist --mode size",
    "analyze:treemap": "npm run build && npx vite-bundle-analyzer dist --mode treemap",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "performance": "lighthouse http://localhost:3002 --output=json --output-path=./lighthouse-report.json",
    "performance:html": "lighthouse http://localhost:3002 --output=html --output-path=./lighthouse-report.html",
    "performance:all": "lighthouse http://localhost:3002 --output=json,html,csv --output-path=./lighthouse-report",
    "bundle-size": "npm run build && npx webpack-bundle-analyzer dist",
    "bundle:check": "npm run build && npx webpack-bundle-analyzer dist",
    "bundle:stats": "npm run build && npx webpack-bundle-analyzer dist --mode static",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rimraf dist node_modules/.cache",
    "clean:all": "rimraf dist node_modules/.cache .nyc_output coverage",
    "dev:analyze": "cross-env ANALYZE=true npm run dev",
    "dev:profile": "cross-env PROFILE=true npm run dev",
    "dev:debug": "cross-env DEBUG=true npm run dev",
    "start": "npm run dev",
    "serve": "npm run preview"
  },
